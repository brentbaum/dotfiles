[user]
	name = Brent Baumgartner
	email = brent@brentbaum.com
[core]
	editor = atom
	autocrlf = input
	excludesfile = /Users/brentbaumgartner/.gitignore_global
[help]
	autocorrect = 1
[push]
	default = simple
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
	helper = osxkeychain
